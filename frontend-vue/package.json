{
  "name": "frontend-vue",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "format": "npx prettier --ignore-path .prettierignore --write '**/*.{css,sass,scss,htm,html,vue,ts,js}'",
    "format:branch": "npx prettier --ignore-path .prettierignore $(git diff --name-only --diff-filter=ACMR $(git merge-base HEAD main) | grep -E '\\.(css|sass|scss|htm|html|vue|ts|js)$' | sed 's|frontend-vue/||') -w -u"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "axios": "^1.10.0",
    "pinia": "^3.0.3",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1",
    "vuetify": "^3.8.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.0",
    "@types/node": "^24.0.7",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.30.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-vue": "^10.2.0",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "eslint-plugin-yaml": "^1.0.3",
    "jiti": "^2.4.2",
    "prettier": "^3.6.2",
    "typescript": "~5.8.3",
    "vite": "^7.0.0",
    "vue-eslint-parser": "^10.1.4",
    "vue-tsc": "^2.2.10",
    "sass": "^1.89.2",
    "vite-plugin-vuetify": "^2.1.1"
  }
}
